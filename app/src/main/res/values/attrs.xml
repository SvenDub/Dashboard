<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SpeedView">
        <attr name="showLabel" format="boolean"/>
        <attr name="unit" format="enum">
            <enum name="ms" value="0"/>
            <enum name="kmh" value="1"/>
            <enum name="mph" value="2"/>
        </attr>
    </declare-styleable>

    <attr name="colorDialer" format="color"/>
    <attr name="colorNavigation" format="color"/>
    <attr name="colorVoice" format="color"/>
</resources>